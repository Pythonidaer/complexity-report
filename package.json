{
  "name": "@pythonidaer/complexity-report",
  "version": "1.0.18",
  "description": "AST-based cyclomatic complexity analyzer with interactive HTML reports and detailed function breakdowns",
  "type": "module",
  "main": "index.js",
  "bin": {
    "complexity-report": "report/cli.js",
    "complexity-analysis": "tools/analysis-cli.js"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "coverage:summary": "node tools/coverage-to-json.js",
    "test:coverage:json": "vitest --coverage && node tools/coverage-to-json.js",
    "decision-points:json": "node tools/decision-points-to-json.js",
    "analysis": "node tools/analysis-cli.js",
    "analysis:merge": "node tools/merge-coverage-decision-points.js",
    "lint": "eslint .",
    "report": "node report/cli.js"
  },
  "files": [
    "index.js",
    "report/",
    "integration/",
    "function-boundaries/",
    "function-extraction/",
    "decision-points/",
    "html-generators/",
    "export-generators/",
    "assets/",
    "tools/",
    "complexity-breakdown.js",
    "function-hierarchy.js",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "keywords": [
    "complexity",
    "cyclomatic-complexity",
    "eslint",
    "code-quality",
    "static-analysis",
    "ast",
    "javascript",
    "typescript",
    "code-metrics",
    "complexity-report"
  ],
  "author": "Johnny Hammond",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pythonidaer/complexity-report.git"
  },
  "homepage": "https://github.com/Pythonidaer/complexity-report#readme",
  "dependencies": {
    "eslint": "^9.0.0",
    "@typescript-eslint/typescript-estree": "^8.0.0"
  },
  "devDependencies": {
    "vitest": "^2.0.0",
    "@vitest/coverage-v8": "^2.0.0"
  },
  "peerDependencies": {
    "eslint": ">=9.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
